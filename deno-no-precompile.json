{
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@testing-library/preact": "npm:@testing-library/preact@^3.2.4",
    "happy-dom": "npm:happy-dom@^18.0.1",
    "preact": "npm:preact@^10.26.9"
  },
  "exclude": [
    ".devcontainer/",
    "node_modules"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "nodeModulesDir": "auto"
}
